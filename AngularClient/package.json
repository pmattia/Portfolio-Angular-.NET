{
  "name": "angular-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "host": "ng serve --host 0.0.0.0",
    "build": "ng build gnappo-lib && gulp --gulpfile ./lib-css.gulpfile.js && ng build WebPortfolio",
    "build-app": "ng build WebPortfolio --localize",
    "build-prod": "ng build gnappo-lib --configuration production && gulp --gulpfile ./lib-css.gulpfile.js && ng build WebPortfolio --configuration production",
    "build-lib": "ng build gnappo-lib && gulp --gulpfile ./lib-css.gulpfile.js",
    "watch": "ng build gnappo-lib --watch --configuration development",
    "test": "ng test",
    "build-sass": "gulp --gulpfile ./lib-css.gulpfile.js",
    "build-lib:stats": "ng build gnappo-lib --stats-json",
    "build-app:stats": "ng build WebPortfolio --stats-json",
    "analyze-lib": "webpack-bundle-analyzer dist/gnappo-lib/stats.json",
    "analyze-app": "webpack-bundle-analyzer dist/web-portfolio/it/stats.json",
    "compodoc": "npx compodoc -p projects/web-portfolio/tsconfig.doc.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@fortawesome/angular-fontawesome": "^0.11.1",
    "@ngrx/effects": "^14.3.2",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "adaptivecards": "^3.0.0-beta.9",
    "botframework-directlinejs": "^0.15.2-master.19bb0c9",
    "crypto-js": "^4.2.0",
    "i": "^0.3.7",
    "ng2-pdf-viewer": "^9.1.3",
    "ngx-device-detector": "^4.0.1",
    "primeflex": "^3.3.0",
    "primeng": "^14.2.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/localize": "^14.2.10",
    "@compodoc/compodoc": "^1.1.19",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "~4.0.0",
    "@types/lodash": "^4.14.189",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-filter": "^7.0.0",
    "gulp-purify-css": "^0.1.0",
    "gulp-sass": "^5.1.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lodash": "^4.17.21",
    "ng-packagr": "^14.1.0",
    "typescript": "~4.7.2",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}
